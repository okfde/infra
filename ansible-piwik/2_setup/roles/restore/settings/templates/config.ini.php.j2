password = "{{ mysql.piwik_pass }}"
dbname = "{{ mysql.piwik_db }}"
tables_prefix = "piwik_"
charset = "utf8"

[General]
assume_secure_protocol = 1
salt = "{{ piwik.mysql_piwik_salt }}"
{% for host in piwik.trusted_hosts %}
